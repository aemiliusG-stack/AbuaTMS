        <span class="nav-label">Dashboard</span>
    </asp:LinkButton>
                        </li>

                        <li>
                            <asp:LinkButton ID="btnPreauthUpdation" runat="server" OnClick="btnPreauthUpdation_Click">
        <i class="fa fa-check"></i> <!-- Preauth Updation uses 'fa-check' -->
